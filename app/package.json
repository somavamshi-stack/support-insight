{
    "name": "support-insight",
    "version": "1.0.0",
    "scripts": {
        "build": "NODE_ENV=production npm run prebuild && next build",
        "build:css": "npx @tailwindcss/cli -i src/styles/globals.css -o src/styles/index.css",
        "build:css:watch": "echo 'Start Watching tailwindcss' && npm run build:css -- --watch",
        "dev": "concurrently \"npm run build:css:watch\" \"npm run start:dev\"",
        "prebuild": "npm run build:css",
        "prettify": "prettier --ignore-path \"./.prettierignore\" --write \"./**/*.{js,jsx,json,html,ts,tsx}\"",
        "start": "next start -H 0.0.0.0 -p 8080",
        "start:dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next dev",
        "preupgrade": "npm i -g npm-check-updates && pnpm remove @parcel/watcher lightningcss sharp @tailwindcss/oxide && rimraf pnpm-lock.yaml",
        "postupgrade": "pnpm add -D @parcel/watcher lightningcss sharp @tailwindcss/oxide && npm run prettify",
        "upgrade": "ncu -u -p pnpm \\!*react \\!*react-dom && pnpm install --silent",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch --watchPathPattern=tests"
    },
